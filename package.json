{
  "name": "keyboard-sounds",
  "displayName": "Keyboard Sounds",
  "description": "Keyboard mechanical sounds for a better typing experience.",
  "version": "1.2.0",
  "publisher": "xfilipe",
  "engines": {
    "vscode": "^1.57.0"
  },
  "homepage": "https://github.com/xfilipe/keyboard-sounds",
  "repository": {
    "type": "git",
    "url": "https://github.com/xfilipe/keyboard-sounds.git"
  },
  "bugs": {
    "url": "https://github.com/xfilipe/keyboard-sounds/issues",
    "email": "filipetamaral@gmail.com"
  },
  "keywords": [
    "sound",
    "keyboard",
    "typing",
    "mechanical",
    "typewriter"
  ],
  "icon": "icon.png",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "xfilipe_keyboard_sounds.activate",
        "title": "Keyboard Sounds: Activate"
      },
      {
        "command": "xfilipe_keyboard_sounds.deactivate",
        "title": "Keyboard Sounds: Deactivate"
      },
      {
        "command": "xfilipe_keyboard_sounds.enable_matrix",
        "title": "Keyboard Sounds: Enable Matrix Sounds"
      },
      {
        "command": "xfilipe_keyboard_sounds.disable_matrix",
        "title": "Keyboard Sounds: Disable Matrix Sounds"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/vscode": "^1.57.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@types/watch": "^1.0.1",
    "path": "^0.12.7"
  }
}

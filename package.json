{
  "name": "keyboard-sounds",
  "displayName": "keyboard-sounds",
  "description": "Keyboard mechanical sounds for a better experience.",
  "version": "0.0.1",
  "publisher": "xfilipe",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:xfilipe_keyboard_sounds.activate"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "xfilipe_keyboard_sounds.activate",
        "title": "Activate Keyboard Sounds"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "memoizee": "^0.3.10",
    "play-sound": "0.0.9"
  }
}
